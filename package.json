{
  "name": "frontlive-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/main.js",
    "watch-node": "nodemon --experimental-specifier-resolution=node dist/main.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch\" \"yarn watch-node\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.0.0",
    "concurrently": "^7.0.0",
    "discord.js": "^14.3.0",
    "discordx": "^11.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "dotenv": "^16.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
